listaDeNomes = ['juliana', 'nami', 'adalberto', 'helliot', 'adalberto']

def acharElemento(array, elemento):
    elementoEncontrato = False

    for i in range(len(array)):
        if (array[i] ==elemento):
            elementoEncontrato = True

    if(encontreiElemento == True):
        print('Encontrei o elemento')
    else:
        print('NÃ£o encontrei o elemento')

acharElemento(listaDeNomes,'douglas')